nav(aria-label='Page navigation')
    ul.pagination.justify-content-end
        li.page-item.disabled
            a.page-link(href='#', tabindex='-1') &laquo; Previous
        li.page-item.active
            a.page-link(href='#') 1
        li.page-item
            a.page-link(href='#') 2
        li.page-item
            a.page-link(href='#') 3
        li.page-item
            a.page-link(href='#') 4
        li.page-item
            a.page-link(href='#') 5
        li.page-item
            a.page-link(href='#') 6
        li.page-item
            a.page-link(href='#') 7
        li.page-item
            a.page-link(href='#') ...
        li.page-item
            a.page-link(href='#') 20
        li.page-item
            a.page-link(href='#') Next &raquo;